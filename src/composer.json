{
  "name": "almasaeed2010/adminlte",
  "description": "AdminLTE - admin control panel and dashboard that's based on Bootstrap 4",
  "homepage": "http://adminlte.io/",
  "keywords": [
    "css",
    "js",
    "less",
    "responsive",
    "back-end",
    "template",
    "theme",
    "web",
    "admin"
  ],
  "authors": [
    {
      "name": "Colorlib"
    }
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/ColorlibHQ/AdminLTE/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "../app/",
      "Src\\": "../src/"
    },
    "files": [
      "../config/config.php"
    ]
  },
  "require": {
    "jenssegers/blade": "^1.3",
    "erandir/plug-route": "^3.7"
  }, 
  "scripts": {
    "serve" : "php -S localhost"
  },
  "config": {
    "process-timeout": 0
  }
}
